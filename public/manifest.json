{
  "name": "Kudos Sender",
  "description": "A simple tool to speed up kudos distribution",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Kudos"
  },
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["https://d.selleo.com/*"],
    "js": ["./static/js/content.js"]
  }]
}
